{
  "manifest_version": 3,
  "name": "Popup closer",
  "version": "2021.10",
      "content_scripts": [
    {
	      "all_frames": true,
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [{
  "resources": ["tab_block.js"],
  "matches": ["<all_urls>"]
}],
   "background": {
 "service_worker": "background.js"
  },
     "permissions": ["tabs", "activeTab","scripting"],
  "action": {
    "default_icon": "close.png",
	   "default_title": "Close popup tabs/windows"
  },	"icons": {
        "128": "close.png"
    }
}
