{
  "manifest_version": 2,
  "name": "Popup closer",
  "version": "1.0",
   "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
     "permissions": ["tabs", "activeTab", "https://*/", "http://*/", "storage"],

  "browser_action": {
    "default_icon": "close.png",
	   "default_title": "Close popup tabs/windows"
  },	"icons": {
        "128": "close.png"
    }
}
